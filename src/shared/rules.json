[
  {
    "id": 1,
    "priority": 1,
    "action": {
      "type": "modifyHeaders",
      "responseHeaders": [
        {
          "header": "cross-origin-resource-policy",
          "operation": "set",
          "value": "cross-origin"
        }
      ]
    },
    "condition": {
      "requestDomains": ["fbcdn.net", "cdninstagram.com"],
      "resourceTypes": [
        "image", "media", "font", "stylesheet", "script",
        "xmlhttprequest", "other", "sub_frame", "main_frame"
      ]
    }
  }
]
